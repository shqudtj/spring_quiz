<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.quiz.pension.dao.BookingMapper">
 	
 	<select id="selectBookingList" resultType="com.quiz.pension.domain.Booking">
 		SELECT
 			`id`
			,`name`
			,`headcount`
			,`day`
			,`date`
			,`phoneNumber`
			,`state`
			,`createdAt`
			,`updatedAt`
		FROM
			`booking`
 	</select>
 
 	<delete id="deleteBookingById" parameterType="int">
 		DELETE
 		FROM
 			`booking`
 		WHERE
 			`id` = #{id}
 	</delete>
 
 </mapper>